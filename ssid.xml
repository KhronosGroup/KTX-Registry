    <!--
    This file, ssid.xml, is the KTX file format Supercompression Scheme Registry.
    The canonical version of the registry, together with documentation can be
    found in the Khronos Registry at
        https://www.github.com/KhronosGroup/KTX-Registry
    -->

    <supercompressionSchemes start="0x00000" end="0x0FFFF" vendor="KHR">
        <id value="0x0000" name="NONE"/>
        <id value="0x0001" name="BasisLZ"/>
        <id value="0x0002" name="Zstandard"/>
        <id value="0x0003" name="ZLIB"/>
    </supercompressionSchemes>

    <!-- The following is an example -->
    <supercompressionSchemes start="0x10000-0x10005" vendor="Acme" comment="Reserved for Wile E. Coyote">
        <id value="0x10001" name="AcmeSuper
        <unused start="0x10002" end="0x10005"/>
    </supercompressionSchemes>
<registry>
